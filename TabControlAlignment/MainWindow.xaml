<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TabControlAlignment"
        xmlns:syncfusion="http://schemas.syncfusion.com/wpf"
        xmlns:Themes="clr-namespace:Microsoft.Windows.Themes;assembly=PresentationFramework.Aero" xmlns:Themes1="clr-namespace:Microsoft.Windows.Themes;assembly=PresentationFramework.Luna" x:Class="TabControlAlignment.MainWindow"
        WindowStartupLocation="CenterScreen"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">


    <Window.Resources>
        <Style x:Key="NewtabControlExtStyle" TargetType="{x:Type syncfusion:TabControlExt}">
            <Setter Property="BorderThickness" Value="1"/>
            <Setter Property="Background" Value="Transparent"/>
            <Setter Property="TabPanelStyle">
                <Setter.Value>
                    <Style TargetType="{x:Type syncfusion:TabPanelAdv}">
                        <Setter Property="Padding" Value="0"/>
                        <Setter Property="Margin" Value="2,2,4,0"/>
                        <Setter Property="SnapsToDevicePixels" Value="True"/>
                        <Setter Property="Template">
                            <Setter.Value>
                                <ControlTemplate TargetType="{x:Type syncfusion:TabPanelAdv}">
                                    <Border x:Name="Bord" BorderBrush="{TemplateBinding BorderBrush}"
                                            BorderThickness="{TemplateBinding BorderThickness}" 
                                            Background="{Binding TabPanelBackground, FallbackValue=Transparent, RelativeSource={RelativeSource FindAncestor, AncestorLevel=1, AncestorType={x:Type syncfusion:TabControlExt}}}" 
                                            CornerRadius="{TemplateBinding CornerRadius}" HorizontalAlignment="Stretch"
                                            Margin="{TemplateBinding Margin}" Padding="{TemplateBinding Padding}" VerticalAlignment="Top">
                                        <DockPanel LastChildFill="True">
                                            <Border x:Name="BottomBorder" Background="Transparent" DockPanel.Dock="Bottom" Height="0"/>
                                            <ContentPresenter x:Name="PART_CustomItems" AllowDrop="False" 
                                                              Content="{Binding TabPanelItem, FallbackValue='', Mode=OneWay, RelativeSource={RelativeSource FindAncestor, AncestorLevel=1, AncestorType={x:Type syncfusion:TabControlExt}}}" 
                                                              ContentSource="TabPanelItem" DockPanel.Dock="Right"
                                                              MaxHeight="{Binding ActualHeight, ElementName=PART_TabItems}" Margin="2,0,0,0"/>
                                            <syncfusion:NewTabLayout>
                                                <ContentPresenter x:Name="PART_ToolBarTray" Content="{Binding ToolBarTray, FallbackValue='', Mode=OneWay, RelativeSource={RelativeSource FindAncestor, AncestorLevel=1, AncestorType={x:Type syncfusion:TabControlExt}}}"/>
                                                <syncfusion:TabItemExt x:Name="PART_NewTab" BorderThickness="80"
                                                                       DockPanel.Dock="Top" IsNewTab="True" Margin="0,0,0,-1"/>
                                            </syncfusion:NewTabLayout>
                                            <ToggleButton x:Name="PART_CloseButton" AllowDrop="False" DockPanel.Dock="Right" Focusable="False" Height="14" Width="14">
                                                <ToggleButton.Command>
                                                    <RoutedCommand/>
                                                </ToggleButton.Command>
                                                <ToggleButton.ToolTip>
                                                    <ToolTip x:Name="tooltip">
                                                        <TextBlock x:Name="tooltipText" Text="{Binding CloseButtonTooltipText}"><Run Text="Close"/></TextBlock>
                                                    </ToolTip>
                                                </ToggleButton.ToolTip>
                                                <ToggleButton.Template>
                                                    <ControlTemplate TargetType="{x:Type ButtonBase}">
                                                        <syncfusion:Border3D x:Name="brdBack" BorderBrush="Transparent" BorderThickness="1">
                                                            <Path x:Name="pathButton" Data="M1,1L7,7 M1,7L7,1" HorizontalAlignment="Center"
                                                                  Height="8" SnapsToDevicePixels="True" Stroke="Black" StrokeThickness="1.5"
                                                                  VerticalAlignment="Center" Width="8"/>
                                                        </syncfusion:Border3D>
                                                        <ControlTemplate.Triggers>
                                                            <Trigger Property="syncfusion:SkinStorage.EnableTouch" Value="True">
                                                                <Setter Property="Height" TargetName="pathButton" Value="13"/>
                                                                <Setter Property="Width" TargetName="pathButton" Value="13"/>
                                                                <Setter Property="Data" TargetName="pathButton" Value="M1,1L12,12 M1,12L12,1"/>
                                                                <Setter Property="StrokeThickness" TargetName="pathButton" Value="2.5"/>
                                                            </Trigger>
                                                            <Trigger Property="IsPressed" Value="True">
                                                                <Setter Property="IsInverted" TargetName="brdBack" Value="True"/>
                                                                <Setter Property="Background" TargetName="brdBack" Value="#FFCEEDFA"/>
                                                                <Setter Property="BorderBrush" TargetName="brdBack" Value="#FF3399FF"/>
                                                                <Setter Property="Stroke" TargetName="pathButton" Value="Black"/>
                                                            </Trigger>
                                                            <MultiTrigger>
                                                                <MultiTrigger.Conditions>
                                                                    <Condition Property="IsMouseOver" Value="True"/>
                                                                    <Condition Property="syncfusion:SkinStorage.EnableTouch" Value="True"/>
                                                                </MultiTrigger.Conditions>
                                                            </MultiTrigger>
                                                            <MultiTrigger>
                                                                <MultiTrigger.Conditions>
                                                                    <Condition Property="IsMouseOver" Value="True"/>
                                                                    <Condition Property="syncfusion:SkinStorage.EnableTouch" Value="False"/>
                                                                </MultiTrigger.Conditions>
                                                                <Setter Property="Background" TargetName="brdBack" Value="#FFCEEDFA"/>
                                                                <Setter Property="BorderBrush" TargetName="brdBack" Value="#FF3399FF"/>
                                                                <Setter Property="Stroke" TargetName="pathButton" Value="Black"/>
                                                            </MultiTrigger>
                                                        </ControlTemplate.Triggers>
                                                    </ControlTemplate>
                                                </ToggleButton.Template>
                                                <ToggleButton.Visibility>
                                                    <Binding FallbackValue="Collapsed" Mode="OneWay" 
                                                             Path="CloseButtonType" 
                                                             RelativeSource="{RelativeSource FindAncestor, AncestorLevel=1, AncestorType={x:Type syncfusion:TabControlExt}}">
                                                        <Binding.Converter>
                                                            <syncfusion:CloseButtonTypeToVisibilityConverter/>
                                                        </Binding.Converter>
                                                    </Binding>
                                                </ToggleButton.Visibility>
                                            </ToggleButton>
                                            <ToggleButton x:Name="PART_MenuButton" AllowDrop="False"
                                                          DockPanel.Dock="Right" Focusable="False" 
                                                          Height="14" Width="14">
                                                <ToggleButton.Command>
                                                    <RoutedCommand/>
                                                </ToggleButton.Command>
                                                <ToggleButton.ToolTip>
                                                    <ToolTip x:Name="PART_ContextMenuButtonTooltip">
                                                        <TextBlock x:Name="PART_ContextMenuButtonTooltipText" 
                                                                   Text="{Binding ContextMenuButtonTooltipText}">
                                                            <Run Text="Context Menu"/>
                                                        </TextBlock>
                                                    </ToolTip>
                                                </ToggleButton.ToolTip>
                                                <ToggleButton.Template>
                                                    <ControlTemplate TargetType="{x:Type ButtonBase}">
                                                        <syncfusion:Border3D x:Name="brdBack" BorderBrush="Transparent" BorderThickness="1">
                                                            <Path x:Name="pathButton" Fill="Black" HorizontalAlignment="Center" Height="8"
                                                                  SnapsToDevicePixels="True" VerticalAlignment="Center" Width="8">
                                                                <Path.Data>
                                                                    <GeometryGroup>
                                                                        <PathGeometry Figures="M0,1 L8,1 L8,3 L0,3 z"/>
                                                                        <PathGeometry Figures="M0,4 L4,8 L8,4 z"/>
                                                                    </GeometryGroup>
                                                                </Path.Data>
                                                            </Path>
                                                        </syncfusion:Border3D>
                                                        <ControlTemplate.Triggers>
                                                            <Trigger Property="syncfusion:SkinStorage.EnableTouch" Value="True">
                                                                <Setter Property="Height" TargetName="pathButton" Value="13"/>
                                                                <Setter Property="Width" TargetName="pathButton" Value="13"/>
                                                                <Setter Property="Data" TargetName="pathButton">
                                                                    <Setter.Value>
                                                                        <GeometryGroup>
                                                                            <PathGeometry Figures="M0,1 L12,1 L12,4 L0,4 z"/>
                                                                            <PathGeometry Figures="M0,6 L6,12 L12,6 z"/>
                                                                        </GeometryGroup>
                                                                    </Setter.Value>
                                                                </Setter>
                                                            </Trigger>
                                                            <MultiDataTrigger>
                                                                <MultiDataTrigger.Conditions>
                                                                    <Condition Binding="{Binding IsAllItemsVisible, RelativeSource={RelativeSource TemplatedParent}}" Value="True"/>
                                                                    <Condition Binding="{Binding (syncfusion:SkinStorage.EnableTouch), FallbackValue=false,
                                                                                RelativeSource={RelativeSource FindAncestor, AncestorLevel=1, AncestorType={x:Type syncfusion:TabControlExt}}}" 
                                                                               Value="false"/>
                                                                </MultiDataTrigger.Conditions>
                                                                <Setter Property="Data" TargetName="pathButton" Value="M0,4L4,8 8,4z"/>
                                                            </MultiDataTrigger>
                                                            <MultiDataTrigger>
                                                                <MultiDataTrigger.Conditions>
                                                                    <Condition Binding="{Binding IsAllItemsVisible, RelativeSource={RelativeSource TemplatedParent}}" Value="True"/>
                                                                    <Condition Binding="{Binding (syncfusion:SkinStorage.EnableTouch), FallbackValue=false,
                                                                                RelativeSource={RelativeSource FindAncestor, AncestorLevel=1, AncestorType={x:Type syncfusion:TabControlExt}}}" Value="true"/>
                                                                </MultiDataTrigger.Conditions>
                                                                <Setter Property="Width" TargetName="pathButton" Value="14"/>
                                                                <Setter Property="Data" TargetName="pathButton" Value="M0,4L8,12 14,4z"/>
                                                            </MultiDataTrigger>
                                                            <Trigger Property="IsPressed" Value="True">
                                                                <Setter Property="IsInverted" TargetName="brdBack" Value="True"/>
                                                                <Setter Property="Background" TargetName="brdBack" Value="#FFCEEDFA"/>
                                                                <Setter Property="BorderBrush" TargetName="brdBack" Value="#FF3399FF"/>
                                                                <Setter Property="Stroke" TargetName="pathButton" Value="Black"/>
                                                            </Trigger>
                                                            <MultiTrigger>
                                                                <MultiTrigger.Conditions>
                                                                    <Condition Property="IsMouseOver" Value="True"/>
                                                                    <Condition Property="syncfusion:SkinStorage.EnableTouch" Value="True"/>
                                                                </MultiTrigger.Conditions>
                                                            </MultiTrigger>
                                                            <MultiTrigger>
                                                                <MultiTrigger.Conditions>
                                                                    <Condition Property="IsMouseOver" Value="True"/>
                                                                    <Condition Property="syncfusion:SkinStorage.EnableTouch" Value="False"/>
                                                                </MultiTrigger.Conditions>
                                                                <Setter Property="Background" TargetName="brdBack" Value="#FFCEEDFA"/>
                                                                <Setter Property="BorderBrush" TargetName="brdBack" Value="#FF3399FF"/>
                                                                <Setter Property="Fill" TargetName="pathButton" Value="Black"/>
                                                            </MultiTrigger>
                                                        </ControlTemplate.Triggers>
                                                    </ControlTemplate>
                                                </ToggleButton.Template>
                                                <ToggleButton.Visibility>
                                                    <Binding FallbackValue="Collapsed" Mode="OneWay" Path="ShowTabListContextMenu"
                                                             RelativeSource="{RelativeSource FindAncestor, AncestorLevel=1, AncestorType={x:Type syncfusion:TabControlExt}}">
                                                        <Binding.Converter>
                                                            <syncfusion:BooleanToVisibilityConverterEx/>
                                                        </Binding.Converter>
                                                    </Binding>
                                                </ToggleButton.Visibility>
                                            </ToggleButton>
                                            <Button x:Name="PART_LastTab" DockPanel.Dock="Right" Focusable="False" Tag="LastTab" Visibility="Collapsed">
                                                <Button.Style>
                                                    <Style TargetType="{x:Type Button}">
                                                        <Setter Property="Visibility" Value="Collapsed"/>
                                                        <Setter Property="Template">
                                                            <Setter.Value>
                                                                <ControlTemplate TargetType="{x:Type Button}">
                                                                    <syncfusion:Border3D x:Name="PART_Border" BorderBrush="Transparent" 
                                                                                         BorderThickness="1" Background="Transparent" 
                                                                                         CornerRadius="0" HorizontalAlignment="Center" Height="15"
                                                                                         IsEnabled="{TemplateBinding IsEnabled}" Margin="1,0,0,0" 
                                                                                         Opacity="{TemplateBinding Opacity}" Padding="1"
                                                                                         SnapsToDevicePixels="True" VerticalAlignment="Center"
                                                                                         Width="15">
                                                                        <Canvas x:Name="canvas" HorizontalAlignment="Center" Height="7" VerticalAlignment="Center" Width="8">
                                                                            <Path x:Name="PART_Path1" Data="M0,0L4,3.5 0,7" Fill="Black" 
                                                                                  HorizontalAlignment="Center" Stroke="Black" VerticalAlignment="Center"/>
                                                                            <Path x:Name="PART_Path2" Data="M4,0L8,3.5 4,7" Fill="Black" 
                                                                                  HorizontalAlignment="Center" Stroke="Black" VerticalAlignment="Center"/>
                                                                            <Path x:Name="PART_Path3" Data="M5,0L5,7"
                                                                                  Fill="Black" HorizontalAlignment="Center" 
                                                                                  Stroke="Black" Visibility="Collapsed" VerticalAlignment="Center"/>
                                                                        </Canvas>
                                                                    </syncfusion:Border3D>
                                                                    <ControlTemplate.Triggers>
                                                                        <Trigger Property="syncfusion:SkinStorage.EnableTouch" Value="True">
                                                                            <Setter Property="Data" TargetName="PART_Path1" Value="M0,0L8,6 0,12"/>
                                                                            <Setter Property="Height" TargetName="PART_Border" Value="24"/>
                                                                            <Setter Property="Width" TargetName="PART_Border" Value="24"/>
                                                                            <Setter Property="Data" TargetName="PART_Path2" Value="M4,0L12,6 4,12"/>
                                                                            <Setter Property="Margin" TargetName="canvas" Value="0,0,0,4"/>
                                                                        </Trigger>
                                                                        <MultiTrigger>
                                                                            <MultiTrigger.Conditions>
                                                                                <Condition Property="syncfusion:SkinStorage.EnableTouch" Value="False"/>
                                                                                <Condition Property="Tag" Value="LastTab"/>
                                                                            </MultiTrigger.Conditions>
                                                                            <Setter Property="Visibility" TargetName="PART_Path2" Value="Collapsed"/>
                                                                            <Setter Property="Visibility" TargetName="PART_Path3" Value="Visible"/>
                                                                        </MultiTrigger>
                                                                        <MultiTrigger>
                                                                            <MultiTrigger.Conditions>
                                                                                <Condition Property="syncfusion:SkinStorage.EnableTouch" Value="True"/>
                                                                                <Condition Property="Tag" Value="LastTab"/>
                                                                            </MultiTrigger.Conditions>
                                                                            <Setter Property="Data" TargetName="PART_Path3" Value="M9,0L9,12"/>
                                                                            <Setter Property="StrokeThickness" TargetName="PART_Path3" Value="2"/>
                                                                            <Setter Property="Visibility" TargetName="PART_Path2" Value="Collapsed"/>
                                                                            <Setter Property="Visibility" TargetName="PART_Path3" Value="Visible"/>
                                                                        </MultiTrigger>
                                                                        <MultiTrigger>
                                                                            <MultiTrigger.Conditions>
                                                                                <Condition Property="syncfusion:SkinStorage.EnableTouch" Value="False"/>
                                                                                <Condition Property="Tag" Value="FirstTab"/>
                                                                            </MultiTrigger.Conditions>
                                                                            <Setter Property="Data" TargetName="PART_Path2" Value="M8,0L4,3.5 8,7"/>
                                                                            <Setter Property="Data" TargetName="PART_Path3" Value="M2,0L2,7"/>
                                                                            <Setter Property="Visibility" TargetName="PART_Path1" Value="Collapsed"/>
                                                                            <Setter Property="Visibility" TargetName="PART_Path3" Value="Visible"/>
                                                                        </MultiTrigger>
                                                                        <MultiTrigger>
                                                                            <MultiTrigger.Conditions>
                                                                                <Condition Property="syncfusion:SkinStorage.EnableTouch" Value="True"/>
                                                                                <Condition Property="Tag" Value="FirstTab"/>
                                                                            </MultiTrigger.Conditions>
                                                                            <Setter Property="Data" TargetName="PART_Path2" Value="M12,0L4,5.5 12,11"/>
                                                                            <Setter Property="Margin" TargetName="canvas" Value="0,0,4,5"/>
                                                                            <Setter Property="Data" TargetName="PART_Path3" Value="M2,0L2,12"/>
                                                                            <Setter Property="StrokeThickness" TargetName="PART_Path3" Value="2"/>
                                                                            <Setter Property="Visibility" TargetName="PART_Path1" Value="Collapsed"/>
                                                                            <Setter Property="Visibility" TargetName="PART_Path3" Value="Visible"/>
                                                                        </MultiTrigger>
                                                                        <MultiTrigger>
                                                                            <MultiTrigger.Conditions>
                                                                                <Condition Property="syncfusion:SkinStorage.EnableTouch" Value="False"/>
                                                                                <Condition Property="Tag" Value="PrevPage"/>
                                                                            </MultiTrigger.Conditions>
                                                                            <Setter Property="Data" TargetName="PART_Path1" Value="M4,0L0,3.5 4,7"/>
                                                                            <Setter Property="Data" TargetName="PART_Path2" Value="M8,0L4,3.5 8,7"/>
                                                                        </MultiTrigger>
                                                                        <MultiTrigger>
                                                                            <MultiTrigger.Conditions>
                                                                                <Condition Property="syncfusion:SkinStorage.EnableTouch" Value="True"/>
                                                                                <Condition Property="Tag" Value="PrevPage"/>
                                                                            </MultiTrigger.Conditions>
                                                                            <Setter Property="Margin" TargetName="canvas" Value="0,0,4,5"/>
                                                                            <Setter Property="Data" TargetName="PART_Path1" Value="M8,0L0,6 8,11"/>
                                                                            <Setter Property="Data" TargetName="PART_Path2" Value="M12,0L4,6 12,11"/>
                                                                        </MultiTrigger>
                                                                        <MultiTrigger>
                                                                            <MultiTrigger.Conditions>
                                                                                <Condition Property="syncfusion:SkinStorage.EnableTouch" Value="True"/>
                                                                                <Condition Property="Tag" Value="NextPage"/>
                                                                            </MultiTrigger.Conditions>
                                                                            <Setter Property="Margin" TargetName="canvas" Value="0,0,4,5"/>
                                                                        </MultiTrigger>
                                                                        <MultiTrigger>
                                                                            <MultiTrigger.Conditions>
                                                                                <Condition Property="syncfusion:SkinStorage.EnableTouch" Value="False"/>
                                                                                <Condition Property="Tag" Value="PrevTab"/>
                                                                            </MultiTrigger.Conditions>
                                                                            <Setter Property="Data" TargetName="PART_Path1" Value="M4,0L0,4 4,8"/>
                                                                            <Setter Property="Visibility" TargetName="PART_Path2" Value="Collapsed"/>
                                                                        </MultiTrigger>
                                                                        <MultiTrigger>
                                                                            <MultiTrigger.Conditions>
                                                                                <Condition Property="syncfusion:SkinStorage.EnableTouch" Value="True"/>
                                                                                <Condition Property="Tag" Value="PrevTab"/>
                                                                            </MultiTrigger.Conditions>
                                                                            <Setter Property="Data" TargetName="PART_Path1" Value="M8,0L0,6 8,12"/>
                                                                            <Setter Property="Visibility" TargetName="PART_Path2" Value="Collapsed"/>
                                                                        </MultiTrigger>
                                                                        <Trigger Property="Tag" Value="NextTab">
                                                                            <Setter Property="Visibility" TargetName="PART_Path2" Value="Collapsed"/>
                                                                        </Trigger>
                                                                        <Trigger Property="IsPressed" Value="True">
                                                                            <Setter Property="IsInverted" TargetName="PART_Border" Value="True"/>
                                                                            <Setter Property="Content" Value="1,1,-1,-1"/>
                                                                            <Setter Property="Background" TargetName="PART_Border" Value="#FFCEEDFA"/>
                                                                            <Setter Property="BorderBrush" TargetName="PART_Border" Value="#FF3399FF"/>
                                                                            <Setter Property="Stroke" TargetName="PART_Path1" Value="Black"/>
                                                                            <Setter Property="Fill" TargetName="PART_Path1" Value="Black"/>
                                                                            <Setter Property="Stroke" TargetName="PART_Path2" Value="Black"/>
                                                                            <Setter Property="Fill" TargetName="PART_Path2" Value="Black"/>
                                                                            <Setter Property="Stroke" TargetName="PART_Path3" Value="Black"/>
                                                                            <Setter Property="Fill" TargetName="PART_Path3" Value="Black"/>
                                                                        </Trigger>
                                                                        <MultiTrigger>
                                                                            <MultiTrigger.Conditions>
                                                                                <Condition Property="syncfusion:SkinStorage.EnableTouch" Value="True"/>
                                                                                <Condition Property="IsMouseOver" Value="True"/>
                                                                            </MultiTrigger.Conditions>
                                                                        </MultiTrigger>
                                                                        <MultiTrigger>
                                                                            <MultiTrigger.Conditions>
                                                                                <Condition Property="syncfusion:SkinStorage.EnableTouch" Value="False"/>
                                                                                <Condition Property="IsMouseOver" Value="True"/>
                                                                            </MultiTrigger.Conditions>
                                                                            <Setter Property="Background" TargetName="PART_Border" Value="#FFCEEDFA"/>
                                                                            <Setter Property="BorderBrush" TargetName="PART_Border" Value="#FF3399FF"/>
                                                                            <Setter Property="Stroke" TargetName="PART_Path1" Value="Black"/>
                                                                            <Setter Property="Fill" TargetName="PART_Path1" Value="Black"/>
                                                                            <Setter Property="Stroke" TargetName="PART_Path2" Value="Black"/>
                                                                            <Setter Property="Fill" TargetName="PART_Path2" Value="Black"/>
                                                                            <Setter Property="Stroke" TargetName="PART_Path3" Value="Black"/>
                                                                            <Setter Property="Fill" TargetName="PART_Path3" Value="Black"/>
                                                                        </MultiTrigger>
                                                                    </ControlTemplate.Triggers>
                                                                </ControlTemplate>
                                                            </Setter.Value>
                                                        </Setter>
                                                    </Style>
                                                </Button.Style>
                                            </Button>
                                            <Button x:Name="PART_NextTab" DockPanel.Dock="Right" Focusable="False" Tag="NextTab" Visibility="Collapsed">
                                                <Button.Style>
                                                    <Style TargetType="{x:Type Button}">
                                                        <Setter Property="Visibility" Value="Collapsed"/>
                                                        <Setter Property="Template">
                                                            <Setter.Value>
                                                                <ControlTemplate TargetType="{x:Type Button}">
                                                                    <syncfusion:Border3D x:Name="PART_Border" 
                                                                                         BorderBrush="Transparent" BorderThickness="1" 
                                                                                         Background="Transparent" CornerRadius="0" HorizontalAlignment="Center" 
                                                                                         Height="15" IsEnabled="{TemplateBinding IsEnabled}" Margin="1,0,0,0" 
                                                                                         Opacity="{TemplateBinding Opacity}" Padding="1" SnapsToDevicePixels="True"
                                                                                         VerticalAlignment="Center" Width="15">
                                                                        <Canvas x:Name="canvas" HorizontalAlignment="Center" Height="7" VerticalAlignment="Center" Width="8">
                                                                            <Path x:Name="PART_Path1" Data="M0,0L4,3.5 0,7" Fill="Black" 
                                                                                  HorizontalAlignment="Center" Stroke="Black" VerticalAlignment="Center"/>
                                                                            <Path x:Name="PART_Path2" Data="M4,0L8,3.5 4,7" Fill="Black" 
                                                                                  HorizontalAlignment="Center" Stroke="Black" VerticalAlignment="Center"/>
                                                                            <Path x:Name="PART_Path3" Data="M5,0L5,7" Fill="Black"
                                                                                  HorizontalAlignment="Center" Stroke="Black" Visibility="Collapsed" VerticalAlignment="Center"/>
                                                                        </Canvas>
                                                                    </syncfusion:Border3D>
                                                                    <ControlTemplate.Triggers>
                                                                        <Trigger Property="syncfusion:SkinStorage.EnableTouch" Value="True">
                                                                            <Setter Property="Data" TargetName="PART_Path1" Value="M0,0L8,6 0,12"/>
                                                                            <Setter Property="Height" TargetName="PART_Border" Value="24"/>
                                                                            <Setter Property="Width" TargetName="PART_Border" Value="24"/>
                                                                            <Setter Property="Data" TargetName="PART_Path2" Value="M4,0L12,6 4,12"/>
                                                                            <Setter Property="Margin" TargetName="canvas" Value="0,0,0,4"/>
                                                                        </Trigger>
                                                                        <MultiTrigger>
                                                                            <MultiTrigger.Conditions>
                                                                                <Condition Property="syncfusion:SkinStorage.EnableTouch" Value="False"/>
                                                                                <Condition Property="Tag" Value="LastTab"/>
                                                                            </MultiTrigger.Conditions>
                                                                            <Setter Property="Visibility" TargetName="PART_Path2" Value="Collapsed"/>
                                                                            <Setter Property="Visibility" TargetName="PART_Path3" Value="Visible"/>
                                                                        </MultiTrigger>
                                                                        <MultiTrigger>
                                                                            <MultiTrigger.Conditions>
                                                                                <Condition Property="syncfusion:SkinStorage.EnableTouch" Value="True"/>
                                                                                <Condition Property="Tag" Value="LastTab"/>
                                                                            </MultiTrigger.Conditions>
                                                                            <Setter Property="Data" TargetName="PART_Path3" Value="M9,0L9,12"/>
                                                                            <Setter Property="StrokeThickness" TargetName="PART_Path3" Value="2"/>
                                                                            <Setter Property="Visibility" TargetName="PART_Path2" Value="Collapsed"/>
                                                                            <Setter Property="Visibility" TargetName="PART_Path3" Value="Visible"/>
                                                                        </MultiTrigger>
                                                                        <MultiTrigger>
                                                                            <MultiTrigger.Conditions>
                                                                                <Condition Property="syncfusion:SkinStorage.EnableTouch" Value="False"/>
                                                                                <Condition Property="Tag" Value="FirstTab"/>
                                                                            </MultiTrigger.Conditions>
                                                                            <Setter Property="Data" TargetName="PART_Path2" Value="M8,0L4,3.5 8,7"/>
                                                                            <Setter Property="Data" TargetName="PART_Path3" Value="M2,0L2,7"/>
                                                                            <Setter Property="Visibility" TargetName="PART_Path1" Value="Collapsed"/>
                                                                            <Setter Property="Visibility" TargetName="PART_Path3" Value="Visible"/>
                                                                        </MultiTrigger>
                                                                        <MultiTrigger>
                                                                            <MultiTrigger.Conditions>
                                                                                <Condition Property="syncfusion:SkinStorage.EnableTouch" Value="True"/>
                                                                                <Condition Property="Tag" Value="FirstTab"/>
                                                                            </MultiTrigger.Conditions>
                                                                            <Setter Property="Data" TargetName="PART_Path2" Value="M12,0L4,5.5 12,11"/>
                                                                            <Setter Property="Margin" TargetName="canvas" Value="0,0,4,5"/>
                                                                            <Setter Property="Data" TargetName="PART_Path3" Value="M2,0L2,12"/>
                                                                            <Setter Property="StrokeThickness" TargetName="PART_Path3" Value="2"/>
                                                                            <Setter Property="Visibility" TargetName="PART_Path1" Value="Collapsed"/>
                                                                            <Setter Property="Visibility" TargetName="PART_Path3" Value="Visible"/>
                                                                        </MultiTrigger>
                                                                        <MultiTrigger>
                                                                            <MultiTrigger.Conditions>
                                                                                <Condition Property="syncfusion:SkinStorage.EnableTouch" Value="False"/>
                                                                                <Condition Property="Tag" Value="PrevPage"/>
                                                                            </MultiTrigger.Conditions>
                                                                            <Setter Property="Data" TargetName="PART_Path1" Value="M4,0L0,3.5 4,7"/>
                                                                            <Setter Property="Data" TargetName="PART_Path2" Value="M8,0L4,3.5 8,7"/>
                                                                        </MultiTrigger>
                                                                        <MultiTrigger>
                                                                            <MultiTrigger.Conditions>
                                                                                <Condition Property="syncfusion:SkinStorage.EnableTouch" Value="True"/>
                                                                                <Condition Property="Tag" Value="PrevPage"/>
                                                                            </MultiTrigger.Conditions>
                                                                            <Setter Property="Margin" TargetName="canvas" Value="0,0,4,5"/>
                                                                            <Setter Property="Data" TargetName="PART_Path1" Value="M8,0L0,6 8,11"/>
                                                                            <Setter Property="Data" TargetName="PART_Path2" Value="M12,0L4,6 12,11"/>
                                                                        </MultiTrigger>
